{
  "modules": {
    "utilities": {
      "config_file": "./utils.json",
      "enabled": true
    },
    "system-users": {
      "enabled": true
    },
    "system": {
      "enabled": true
    },
    "storage": {
      "enabled": true
    },
    "storage-smart": {
      "enabled": true
    },
    "system-logs": {
      "config_file": "logs.json",
      "enabled": true
    },
    "network": {
      "enabled": true
    },
    "network-smb": {
      "enabled": true
    },
    "network-nfs": {
      "enabled": true
    },
    "files": {
      "config_file": "./browser.json",
      "enabled": true
    },
    "mpd": {
      "enabled": true,
			"configuration" : {
				"host": "localhost:6600"
			}
    },
    "notepad": {
      "enabled": true, 
			"configuration": {
			  "dir": "./notepad/"
			}
    },
    "system-other": {
      "enabled": true
    },
    "system-process": {
      "enabled": true
    }
  },
  "StaticDir": "./static",
  "Users": "./users.json",
  "CookieAuthKey": "12345678901234567890123456789012",
  "CookieEncKey": "12345678901234567890012345678901",
  "Debug": true,
  "LogFilename": "app.log",
  "monitor": {
    "cpu_temp_file": "/sys/class/thermal/thermal_zone0/temp",
    "cpu_freq_file": "/sys/devices/system/cpu/cpu0/cpufreq/cpuinfo_cur_freq",
    "cpu_temp_error": 80,
    "monitored_services": {
      "8200": "MiniDLNA Connected",
      "445": "SAMBA Connected",
      "21": "FTP Connected",
      "22": "SSH Connected"
    },
    "load_warning": 0,
    "load_error": 0,
    "ram_usage_warning": 75,
    "swap_usage_warning": 50,
    "fs_usage_warning": 25,
    "fs_usage_error": 15,
    "cpu_temp_warning": 70
  },
  "TemplatesDir": "./templates",
  "MonitorUpdateInterval": 5,
  "SslKey": "key.pem",
  "SslCert": "cert.pem",
  "http_address": ":8000",
  "https_address": "",
  "SessionStoreDir": "./temp"
}
