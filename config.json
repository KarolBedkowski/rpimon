{
  "LogFilename": "app.log",
  "SessionStoreDir": "./temp",
  "CookieEncKey": "12345678901234567890012345678901",
  "CookieAuthKey": "12345678901234567890123456789012",
  "Debug": true,
  "Users": "./users.json",
  "modules": {
    "storage": {
      "enabled": "yes"
    },
    "preferences": {
      "enabled": "yes"
    },
    "notepad": {
      "dir": "./notepad/",
      "enabled": "yes"
    },
    "network-smb": {
      "enabled": "yes"
    },
    "network-nfs": {
      "enabled": "yes"
    },
    "network": {
      "enabled": "yes"
    },
    "mpd": {
      "enabled": "yes",
      "host": "localhost:6600"
    },
    "utilities": {
      "config_file": "./utils.json",
      "enabled": "yes"
    },
    "system-users": {
      "enabled": "yes"
    },
    "system-process": {
      "enabled": "yes"
    },
    "system-other": {
      "enabled": "yes"
    },
    "system-logs": {
      "config_file": "logs.json",
      "enabled": "yes"
    },
    "system": {
      "enabled": "yes"
    },
    "storage-smart": {
      "enabled": "yes"
    },
    "files": {
      "config_file": "./browser.json",
      "enabled": "yes"
    }
  },
  "monitor": {
    "cpu_temp_warning": 70,
    "fs_usage_error": 90,
    "fs_usage_warning": 80,
    "swap_usage_warning": 50,
    "ram_usage_warning": 75,
    "cpu_temp_file": "/sys/class/thermal/thermal_zone0/temp",
    "cpu_freq_file": "/sys/devices/system/cpu/cpu0/cpufreq/cpuinfo_cur_freq",
    "monitored_services": [
      {
        "port": 8200,
        "name": "MiniDLNA Connected"
      },
      {
        "port": 445,
        "name": "SAMBA Connected"
      },
      {
        "port": 22,
        "name": "SSH Connected"
      },
      {
        "port": 21,
        "name": "FTP Connected"
      },
      {
        "port": 33,
        "name": "Connection to port 33"
      }
    ],
    "monitored_hosts": [
      {
        "alarm": 1,
        "interval": 15,
        "method": "ping",
        "address": "nas",
        "name": "nas"
      },
      {
        "alarm": 2,
        "interval": 15,
        "method": "ping",
        "address": "pi",
        "name": "pi"
      },
      {
        "alarm": 3,
        "interval": 15,
        "method": "tcp",
        "address": "192.168.0.1:80",
        "name": "router"
      },
      {
        "alarm": 3,
        "interval": 10,
        "method": "tcp",
        "address": "localhost:8111",
        "name": "localhost tcp"
      }
    ],
    "cpu_temp_error": 80,
    "update_interval": 5,
    "load_warning": 4,
    "load_error": 8
  },
  "SslKey": "key.pem",
  "SslCert": "cert.pem",
  "https_address": "",
  "http_address": ":8000",
  "StaticDir": "./static",
  "TemplatesDir": "./templates"
}
